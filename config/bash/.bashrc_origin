### .bashrc_origin is sourced from .bashrc. ###

source "$HOME"/.dotfiles/lib/path.sh
source "$HOME"/.dotfiles/lib/packages.sh

alias check='shellcheck'
alias relogin='exec $SHELL -l'
alias vi='nvim'

### add path manage ###
PATH="$PATH:$HOME/.dotfiles/bin:$HOME/.dotfiles/bin"
remove_duplicates
export PATH
